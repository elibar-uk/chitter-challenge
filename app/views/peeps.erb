

<div class='centre'>
<h1> Peeps </h1>
<ul id='peeps'>
  <% @peeps.each do |peep| %>
  <li>
    <%= peep.user.username %>
    Title: <%= peep.title %>
    Text:   <%= peep.text   %>
    Context: <%= peep.context  %>
    Date: <%= peep.date  %>
    Time: <%= peep.time  %>
  </li>
<% end %>
</ul>
<% if current_user %>
<form action='/peeps/new'>
<input type='submit' value='add new peep'>
</form>
<% end %>
</div>
